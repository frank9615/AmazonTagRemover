{
    "name": "AmazonTagRemover",
    "version": "1.0",
    "description": "Amazon Tag Remover",
    "permissions": [
        "tabs",
        "activeTab",
        "webNavigation",
        "webRequest",
        "webRequestBlocking",
        "*://*.amazon.ca/*",
        "*://*.amazon.co.jp/*",
        "*://*.amazon.co.uk/*",
        "*://*.amazon.com.au/*",
        "*://*.amazon.com.br/*",
        "*://*.amazon.com.mx/*",
        "*://*.amazon.com/*",
        "*://*.amazon.de/*",
        "*://*.amazon.es/*",
        "*://*.amazon.fr/*",
        "*://*.amazon.in/*",
        "*://*.amazon.it/*",
        "*://*.amazon.nl/*",
        "*://*.amzn.com/*"
    ],
    "author": "Francesco Accardi",
    "browser_action": {
        "default_icon": {
            "16": "images/ico16.png",
            "24": "images/ico24.png",
            "32": "images/ico32.png",
            "64": "images/ico64.png",
            "128": "images/ico128.png",
            "192": "images/ico192.png"
        }
    },
    "icons": {
        "16": "images/ico16.png",
        "24": "images/ico24.png",
        "32": "images/ico32.png",
        "64": "images/ico64.png",
        "128": "images/ico128.png",
        "192": "images/ico192.png"
    },
    "background": {
        "scripts": [
            "src/antitag.js"
        ],
        "persistent": true
    },
    "manifest_version": 2
}